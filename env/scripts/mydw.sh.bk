#!/bin/bash
$op=$1
echo "op:${op}"
$kuduport=$2
echo "kuduport:${kuduport}"

if [ $1 == "start" ]; then
if [ $1 == "7052" ]; then
mykudu_master.sh
~/scripts/myprestoserver.sh start
else
mykudu2_master.sh
~/scripts/myprestoserver2.sh start
fi
else
~/scripts/killport.sh $kuduport
netstat -nlap|grep $kuduport
if [ $1 == "7052" ]; then
~/scripts/myprestoserver.sh stop
else
~/scripts/myprestoserver2.sh stop
fi
fi
